{
  "name": "td-js-sdk",
  "version": "2.0.0-alpha.0",
  "license": "Apache-2.0",
  "bugs": "https://github.com/treasure-data/td-js-sdk/issues",
  "description": "Browser JS library for sending events to your Treasure Data account",
  "main": "src/treasure.js",
  "author": {
    "name": "Cesar Andreu",
    "email": "cesar@treasure-data.com",
    "url": "https://www.treasuredata.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/treasure-data/td-js-sdk"
  },
  "scripts": {
    "build-all": "npm run build-bundle && npm run build-min && npm run build-loader",
    "build-bundle": "webpack",
    "build-loader": "bash ./bin/build-loader.sh",
    "build-min": "webpack --env.build",
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "lint": "standard",
    "test": "standard && npm run test-cloud",
    "test-debug": "zuul --local 9090 --no-coverage --open 'src/**/*.test.js'",
    "test-local": "zuul --local 9090 --open 'src/**/*.test.js'",
    "test-cloud": "zuul --concurrency 1 'src/**/*.test.js'"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "devDependencies": {
    "google-closure-compiler-js": "^20160828.0.1",
    "standard": "^8.0.0",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6",
    "webpack": "^2.1.0-beta.22",
    "zuul": "^3.11.0"
  },
  "dependencies": {
    "domready": "^0.3.0",
    "global": "^4.3.0",
    "js-cookie": "^2.1.3"
  }
}
